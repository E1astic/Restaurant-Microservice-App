<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="v1-create-table-order_position" author="Roman Filimonov">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="order_position"/>
            </not>
        </preConditions>
        <sqlFile path="tables/v1-create-table-order_position.sql" relativeToChangelogFile="true"/>
        <rollback>
            <sqlFile path="tables/rollback/v1-drop-table-order_position.sql" relativeToChangelogFile="true"/>
        </rollback>
    </changeSet>
</databaseChangeLog>